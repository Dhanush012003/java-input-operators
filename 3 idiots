import java.util.Scanner;
class Main
{
public static void main(String args[])
{
// get input
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int c=sc.nextInt();
    int d=sc.nextInt();
    float e=((a+c)/2f);
    float f=((b+d)/2f);
    System.out.println("Binoy's house is located at "+"("+e+","+f+")");
}
}
