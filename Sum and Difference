import java.util.Scanner;
class Main
{
    public static void main(String args[])
        {
            // getting input from user
            Scanner sc=new Scanner(System.in);
            int a=sc.nextInt();
            int b=sc.nextInt();
            
            //adding two numbers
            System.out.println(a+b);
            //subtracting two numbers
            System.out.println(a-b);
            
        }
}
